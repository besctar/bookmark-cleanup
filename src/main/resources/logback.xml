<configuration>

    <appender name="FILE" class="ch.qos.logback.core.FileAppender">
        <file>${USERPROFILE}/Documents/Bookmarks_Output/bookmark-cleanup.log</file>
        <encoder>
            <pattern>%d{HH:mm:ss.S} %-5level %msg%n</pattern>
        </encoder>
    </appender>
    <root level="info">
        <appender-ref ref="FILE" />
    </root>
</configuration>